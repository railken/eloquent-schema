{
    "name": "railken/eloquent-schema",
    "type": "library",
    "keywords": ["laravel", "eloquent-schema"],
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "wyrihaximus/list-classes-in-directory": "^1.6",
        "doctrine/dbal": "^4.0",
        "haydenpierce/class-finder": "^0.5.3",
        "ajthinking/archetype": "^2.0",
        "railken/template": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "orchestra/testbench": "*",
        "friendsofphp/php-cs-fixer": "^3.52",
        "kitloong/laravel-migrations-generator": "^7.0"
    },
    "autoload": {
        "psr-4" : {
            "Railken\\EloquentSchema\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { 
            "Tests\\": "tests/" 
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Railken\\EloquentSchema\\EloquentSchemaServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
